{
  "name": "maghead/laravel-bridge",
  "description": "A Maghead ORM bridge for Laravel 5",
  "license": "MIT",
  "keywords": [
    "doctrine",
    "maghead",
    "laravel",
    "orm",
    "data mapper",
    "database"
  ],
  "authors": [
    { "name": "Yo-An Lin", "email": "yoanlin93@gmail.com" }
  ],
  "require": {
    "php": ">=7.0",
    "maghead/maghead": "4.0.x-dev",
    "corneltek/cliframework": "4.0.x-dev",
    "corneltek/codegen": "3.1.x-dev",
    "illuminate/auth": "5.4.*",
    "illuminate/console": "5.4.*",
    "illuminate/container": "5.4.*",
    "illuminate/contracts": "5.4.*",
    "illuminate/pagination": "5.4.*",
    "illuminate/routing": "5.4.*",
    "illuminate/support": "5.4.*",
    "illuminate/validation": "5.4.*",
    "illuminate/view": "5.4.*",
    "symfony/serializer": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "1.0.0-alpha1",
    "barryvdh/laravel-debugbar": "~2.0",
    "itsgoingd/clockwork": "~1.9",
    "illuminate/log": "5.4.*",
    "illuminate/notifications": "5.4.*",
    "illuminate/queue": "5.4.*"
  },
  "autoload": {
    "psr-4": {
      "Maghead\\Laravel\\": "src/"
    },
    "files": [ ]
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  }
}
